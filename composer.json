{
    "name": "atelierspierrot/webservices",
    "type": "library",
    "description": "A PHP engine to manage web-services easily",
    "keywords": [ "webservices", "engine" ],
    "homepage": "http://github.com/atelierspierrot/webservices",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    },{
        "name": "Pierre Cassat",
        "homepage": "http://e-piwi.fr/"
    }],
    "require": {
        "php": ">=5.3.0",
        "atelierspierrot/patterns": "~1.0.9",
        "atelierspierrot/library": "1.*",
        "piwi/markdown-extended": "dev-master",
        "psr/log": "dev-master"
    },
    "require-dev": {
        "sami/sami": "@stable",
        "fabpot/php-cs-fixer": "@stable",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": { "psr-4": {"WebServices\\": "src/WebServices/"} },
    "scripts": {
        "build-doc": "php vendor/bin/sami.php update .sami.php",
        "update-doc": "php vendor/bin/sami.php update .sami.php",
        "cs-fixer": "php vendor/bin/php-cs-fixer fix -v"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
